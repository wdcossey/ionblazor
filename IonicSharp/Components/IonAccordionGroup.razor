@inherits IonComponent

<ion-accordion-group @ref="@_self"
                     @attributes="@Attributes"
                     animated="@Animated"
                     class="@Class"
                     disabled="@Disabled"
                     expand="@Expand?.ToString().ToLowerInvariant()"
                     mode="@Mode"
                     multiple="@Multiple"
                     readonly="@Readonly">
    @ChildContent
</ion-accordion-group>