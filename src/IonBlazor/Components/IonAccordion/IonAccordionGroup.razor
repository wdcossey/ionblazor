@namespace IonBlazor.Components
@inherits IonContentComponent

<ion-accordion-group @ref="@_self"
                     @attributes="@Attributes"
                     animated="@Animated"
                     disabled="@Disabled"
                     expand="@Expand"
                     mode="@Mode"
                     multiple="@Multiple"
                     readonly="@Readonly">
    <CascadingValue TValue="@IIonComponent" Name="@(nameof(IonAccordion.Parent))" Value="@(this)" IsFixed="@true">
        @ChildContent
    </CascadingValue>
</ion-accordion-group>