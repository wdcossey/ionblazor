@inherits IonComponent

<CascadingValue Name="ion-picker-column" TValue="@IonPickerColumn" Value="@this">
    <ion-picker-column
        @ref="_self"
        class="@Class"
        color="@Color"
        disabled="@Disabled"
        mode="@Mode"
        value="@Value">
        @ChildContent
    </ion-picker-column>
</CascadingValue>