@inherits IonList
@typeparam TItem where TItem: notnull

<ion-list @ref="@Self"
          @attributes="@Attributes"
          class="@Class"
          inset="@Inset"
          lines="@Lines"
          mode="@Mode">
    @foreach (var item in ItemsSource)
    {
        @ItemTemplate(item)
    }
</ion-list>