<IonListOf TItem="ListOfItem" ItemsSource="@_itemsSource">
    <ItemTemplate>
        <IonItem>
            <IonLabel>@context.Title</IonLabel>
        </IonItem>
    </ItemTemplate>
</IonListOf>

@code {
    private readonly IEnumerable<ListOfItem> _itemsSource =
    [
        new() { Title = "Pokémon Yellow" },
        new() { Title = "Mega Man X" },
        new() { Title = "The Legend of Zelda" },
        new() { Title = "Pac-Man" },
        new() { Title = "Super Mario World" }
    ];

    public class ListOfItem
    {
        public required string Title { get; init; }
    }
}