<IonFab slot="fixed" Horizontal="@IonFabHorizontalAlignment.Center" Vertical="@IonFabVerticalAlignment.Center">
    <IonFabButton Color="@IonColor.Secondary">
        <IonIcon Name="add"></IonIcon>
    </IonFabButton>
    <IonFabList Side="@IonFabListSide.Top">
        <IonFabButton Color="@IonColor.Primary">
            <IonIcon name="chevron-up"></IonIcon>
        </IonFabButton>
    </IonFabList>
    <IonFabList Side="@IonFabListSide.End">
        <IonFabButton Color="@IonColor.Tertiary">
            <IonIcon Name="chevron-forward"></IonIcon>
        </IonFabButton>
        <IonFabButton Color="@IonColor.Success">
            <IonIcon Name="chevron-forward"></IonIcon>
        </IonFabButton>
    </IonFabList>
    <IonFabList Side="@IonFabListSide.Bottom">
        <IonFabButton Color="@IonColor.Warning">
            <IonIcon Name="chevron-down"></IonIcon>
        </IonFabButton>
    </IonFabList>
    <IonFabList Side="@IonFabListSide.Start">
        <IonFabButton Color="@IonColor.Danger">
            <IonIcon Name="chevron-back"></IonIcon>
        </IonFabButton>
        <IonFabButton Color="@IonColor.Dark">
            <IonIcon Name="chevron-back"></IonIcon>
        </IonFabButton>
    </IonFabList>
</IonFab>