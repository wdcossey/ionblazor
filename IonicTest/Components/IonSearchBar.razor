@inherits IonControl
<ion-searchbar @ref="_self"
               @attributes="@Attributes"
               animated="@Animated"
               autocomplete="@(AutoComplete ?? SearchBarAutoComplete.Off)"
               autocorrect="@(AutoCorrect is true ? "on" : "off")"
               cancel-button-icon="@CancelButtonIcon"
               cancel-button-text="@CancelButtonText"
               class="@Class"
               clear-icon="@ClearIcon"
               color="@Color"
               debounce="@Debounce"
               disabled="@Disabled"
               enterkeyhint="@EnterKeyHint?.ToString().ToLowerInvariant()"
               inputmode="@InputMode?.ToString().ToLowerInvariant()"
               mode="@Mode?.ToString().ToLowerInvariant()"
               placeholder="@((MarkupString)(Placeholder ?? string.Empty))"
               search-icon="@SearchIcon"
               show-cancel-button="@ShowCancelButton?.ToString().ToLowerInvariant()"
               show-clear-button="@ShowClearButton?.ToString().ToLowerInvariant()"
               spellcheck="@SpellCheck"
               type="@(Type?.ToString().ToLowerInvariant())"
               value="@Value">
</ion-searchbar>