@inherits IonControl

<ion-fab-button @ref="_self"
                @attributes="@Attributes"
                activated="@Activated"
                class="@Class"
                close-icon="@CloseIcon"
                color="@Color"
                disabled="@Disabled"
                download="@Download"
                href="@Href"
                mode="@Mode"
                rel="@Rel"
                router-direction="@RouterDirection"
                show="@Show"
                size="@Size"
                target="@Target"
                translucent="@Translucent"
                type="@Type">
    @ChildContent
</ion-fab-button>